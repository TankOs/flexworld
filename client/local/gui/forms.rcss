button {
	display:inline-block;
	height:30px;
	line-height:24px;
	text-align:center;
	margin:5px 0;
	font-size: 14;

	bg-decorator:tiled-horizontal;
	bg-left-image:forms.png 0px 0px 51px 24px;
	bg-center-image:forms.png repeat-truncate 52px 0px 117px 24px;
	bg-right-image:forms.png 118px 0px 180px 24px;

	color:#000;
}

button:hover {
	shadow-font-effect:shadow;
	shadow-offset:2px 2px;
	shadow-color:#00000033;
}

button:active {
	shadow-font-effect:shadow;
	shadow-offset:2px 2px;
	shadow-color: transparent;

	bg-decorator:tiled-horizontal;
	bg-left-image:forms.png 0px 25px 51px 49px;
	bg-center-image:forms.png repeat-truncate 52px 25px 117px 49px;
	bg-right-image:forms.png 118px 25px 180px 49px;
}

.window {
	position: absolute;
	min-width: 50px;
	min-height: 50px;
	padding: 65px 40px 40px 40px;
	/*margin-top:-65px;
	margin-left:-40px;*/

	bg-decorator:tiled-box;

	bg-top-left-image:forms.png 0px 128px 40px 216px;
	bg-top-right-image:forms.png 405px 128px 498px 216px;
	bg-bottom-right-image:forms.png 405px 255px 498px 308px;
	bg-bottom-left-image:forms.png 0px 255px 84px 308px;

	bg-top-image:forms.png repeat-stretch 85px 128px 404px 216px;
	bg-right-image:forms.png repeat-stretch 405px 217px 498px 254px;
	bg-bottom-image:forms.png repeat-stretch 85px 255px 404px 308px;
	bg-left-image:forms.png repeat-stretch 0px 217px 84px 254px;

	bg-center-image:forms.png repeat-truncate 85px 217px 404px 254px;
}

.window title {
	color:#fff;
	position:absolute;
	text-align:center;
	top:5px;
	left:0px;
	width:100%;
	line-height:25px;
}

tabset tab {
	display:inline-block;
	height:30px;
	line-height:25px;
	text-align:center;
	color:#000;

	margin-right:5px;

	bg-decorator:tiled-horizontal;
	bg-left-image:forms.png 0px 0px 51px 24px;
	bg-center-image:forms.png repeat-truncate 52px 0px 117px 24px;
	bg-right-image:forms.png 118px 0px 180px 24px;
}

tabset tab:hover {
	shadow-font-effect:shadow;
	shadow-offset:2px 2px;
	shadow-color:#00000033;
}

tabset tab:active {
	shadow-font-effect:shadow;
	shadow-offset:2px 2px;
	shadow-color: transparent;

	bg-decorator:tiled-horizontal;
	bg-left-image:forms.png 0px 25px 51px 49px;
	bg-center-image:forms.png repeat-truncate 52px 25px 117px 49px;
	bg-right-image:forms.png 118px 25px 180px 49px;
}

tabset tab:selected {
	color:#00f;
}

tabset panel {
	margin-top: 0px;
}

input.text {
	height: 32px;
	padding: 7px 7px 0 7px;
	/*font-size: 14;*/
	margin-bottom: 5px;
	color:#000;

	bg-decorator:tiled-box;

	bg-top-left-image:forms.png     241px 17px 248px 24px;
	bg-top-right-image:forms.png    251px 17px 258px 24px;
	bg-bottom-right-image:forms.png 251px 27px 258px 34px;
	bg-bottom-left-image:forms.png  241px 27px 248px 34px;

	bg-top-image:forms.png repeat-stretch    248px 17px 251px 24px;
	bg-right-image:forms.png repeat-stretch  251px 24px 258px 27px;
	bg-bottom-image:forms.png repeat-stretch 248px 27px 251px 34px;
	bg-left-image:forms.png repeat-stretch   241px 24px 248px 27px;

	bg-center-image:forms.png repeat-truncate 248px 24px 251px 27px;
}

input.text:focus {
	bg-decorator:tiled-box;

	bg-top-left-image:forms.png     224px 17px 231px 24px;
	bg-top-right-image:forms.png    234px 17px 241px 24px;
	bg-bottom-right-image:forms.png 234px 27px 241px 34px;
	bg-bottom-left-image:forms.png  224px 27px 231px 34px;

	bg-top-image:forms.png repeat-stretch    231px 17px 234px 24px;
	bg-right-image:forms.png repeat-stretch  234px 24px 241px 27px;
	bg-bottom-image:forms.png repeat-stretch 231px 27px 234px 34px;
	bg-left-image:forms.png repeat-stretch   224px 24px 231px 27px;

	bg-center-image:forms.png repeat-truncate 231px 24px 234px 27px;
}

select {
	height: 32px;
	padding: 7px 7px 0 7px;
	/*font-size: 14;*/
	margin-bottom: 5px;

	bg-decorator:tiled-box;

	bg-top-left-image:forms.png     241px 17px 248px 24px;
	bg-top-right-image:forms.png    251px 17px 258px 24px;
	bg-bottom-right-image:forms.png 251px 27px 258px 34px;
	bg-bottom-left-image:forms.png  241px 27px 248px 34px;

	bg-top-image:forms.png repeat-stretch    248px 17px 251px 24px;
	bg-right-image:forms.png repeat-stretch  251px 24px 258px 27px;
	bg-bottom-image:forms.png repeat-stretch 248px 27px 251px 34px;
	bg-left-image:forms.png repeat-stretch   241px 24px 248px 27px;

	bg-center-image:forms.png repeat-truncate 248px 24px 251px 27px;
}

select:focus {
	bg-decorator:tiled-box;

	bg-top-left-image:forms.png     224px 17px 231px 24px;
	bg-top-right-image:forms.png    234px 17px 241px 24px;
	bg-bottom-right-image:forms.png 234px 27px 241px 34px;
	bg-bottom-left-image:forms.png  224px 27px 231px 34px;

	bg-top-image:forms.png repeat-stretch    231px 17px 234px 24px;
	bg-right-image:forms.png repeat-stretch  234px 24px 241px 27px;
	bg-bottom-image:forms.png repeat-stretch 231px 27px 234px 34px;
	bg-left-image:forms.png repeat-stretch   224px 24px 231px 27px;

