select selectarrow {
	width:18px;
	height:18px;

	img-decorator:image;
	img-image:forms.png 192px 20px 207px 35px;
}

select selectbox {
	background:#000000dd;
	color:#fff;
	padding:7px;
	max-height:150px;
	overflow-y:auto;
}

select option:hover {
	background:#ffffffbb;
	color:#000;
}

input.checkbox {
	width:10px;
	height:10px;
	margin-right:5px;

	icon-decorator:image;
	icon-image:forms.png 181px 0px 192px 11px;
}

input.checkbox:hover {
	background-color:#fff3;
}

input.checkbox:checked {
	icon-decorator:image;
	icon-image:forms.png 192px 0px 203px 11px;
}

input.range {
	display:inline-block;
	height:17px;
}

input.range sliderarrowdec {
	display:none;
	width:0;
}

input.range sliderarrowinc {
	display:none;
	width:0;
}

input.range slidertrack {
	height:17px;

	img-decorator:tiled-horizontal;
	img-left-image:forms.png 224px 0px 241px 17px;
	img-center-image:forms.png repeat-truncate 239px 0px 243px 17px;
	img-right-image:forms.png 241px 0px 258px 17px;
}

input.range sliderbar {
	width:17px;
	height:17px;
	img-decorator:image;
	img-image:forms.png 192px 20px 207px 35px;
}

scrollbarvertical {
	width:17px;
}

scrollbarvertical sliderarrowdec {
	display:none;
	width:9px;
	height:0;
	margin-left:4px;
	img-decorator:image;
	img-image:forms.png 181px 11px 190px 20px;
}

scrollbarvertical sliderarrowinc {
	display:none;
	width:9px;
	height:0;
	margin-left:4px;
	img-decorator:image;
	img-image:forms.png 181px 11px 190px 20px;
}

scrollbarvertical sliderbar {
	width:17px;
	height:17px;
	img-decorator:image;
	img-image:forms.png 192px 20px 207px 35px;
}

scrollbarvertical slidertrack {
	width:17px;

	img-decorator:tiled-vertical;
	/*img-top-image:forms.png 207px 15px 224px 19px;
	img-center-image:forms.png repeat-truncate 207px 15px 224px 19px;
	img-bottom-image:forms.png 207px 15px 224px 19px;*/
	img-top-image:forms.png 207px 0px 224px 17px;
	img-center-image:forms.png repeat-truncate 207px 15px 224px 19px;
	img-bottom-image:forms.png 207px 17px 224px 34px;
}
